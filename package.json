{
  "name": "githubhomepage",
  "version": "1.0.0",
  "description": "My Github homepage",
  "main": "index.js",
  "dependencies": {
    "gh-pages": "^2.1.1",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/react": "^9.2.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "xml-loader": "^1.2.1"
  },
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect-brk=0.0.0.0 node_modules/.bin/jest --runInBand",
    "start": "webpack-dev-server --open --hot --mode development",
    "lint": "eslint",
    "lint:all": "eslint src",
    "build": "webpack --mode production",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkasputis/mkasputis.github.io.git"
  },
  "keywords": [
    "react",
    "kasputis"
  ],
  "author": "Matthew Kasputis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mkasputis/mkasputis.github.io/issues"
  },
  "homepage": "https://github.com/mkasputis/mkasputis.github.io#readme"
}
